    "lucide-react": "^0.469.0",
    "osmojs": "^16.15.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-ga4": "^2.1.0",
    "react-hook-form": "^7.50.0",
    "react-router-dom": "^6.17.0",
    "react-scripts": "5.0.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.2",
    "util": "^0.12.5",
    "uuid": "^9.0.1",
    "viem": "^2.21.49",
